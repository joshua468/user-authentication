{
  "version": 2,
  "builds": [
    {
      "src": "main.go",
      "use": "@vercel/go"
    }
  ],
  "routes": [
    {
      "src": "/api/auth/(.*)",
      "dest": "/api/auth"
    },
    {
      "src": "/api/users/(.*)",
      "dest": "/api/users"
    },
    {
      "src": "/api/organisations/(.*)",
      "dest": "/api/organisations"
    },
    {
      "src": "/(.*)",
      "dest": "/main.go"
    }
  ]
}
